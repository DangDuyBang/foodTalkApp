{
  "expo": {
    "name": "foodTalk",
    "slug": "foodTalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/ImageDefault/icon.png",
    "splash": {
      "image": "./src/assets/ImageDefault/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/ImageDefault/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/ImageDefault/icon.png"
    },
    "entryPoint": "./src/index.js"
  }
}
